{
    "name": "core-backend",
    "version": "1.0.0",
    "scripts": {
        "start": "node ./dist/app.js",
        "preprod": "npm run build && npm start",
        "watch": "nodemon --signal SIGTERM src/app.ts",
        "test": "npm run build && mocha tests/*.js",
        "typesync": "typesync package.json && npm i",
        "build": "npm run build:protos && tsc --pretty && npm run build:protos:dist",
        "build:clear": "del-cli --force ./dist/* !./dist/.gitignore !./dist/types ./dist/types/*",
        "build:protos": "npm run build:protos:js && npm run build:protos:ts",
        "build:protos:dist": "grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./dist/types --grpc_out=./dist/types --plugin=protoc-gen-grpc=./node_modules/grpc-tools/bin/grpc_node_plugin -I ../protos ../protos/*.proto",
        "build:protos:js": "grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./src/types --grpc_out=./src/types --plugin=protoc-gen-grpc=./node_modules/grpc-tools/bin/grpc_node_plugin -I ../protos ../protos/*.proto",
        "build:protos:ts": "protoc --plugin=protoc-gen-ts=./node_modules/grpc_tools_node_protoc_ts/bin/protoc-gen-ts --ts_out=./src/types -I ../protos ../protos/*.proto"
    },
    "author": "Garen Yondem",
    "license": "MIT",
    "dependencies": {
        "@slack/web-api": "^5.6.0",
        "aws-sdk": "^2.602.0",
        "bcrypt": "^3.0.7",
        "csvtojson": "^2.0.10",
        "dotenv": "^8.2.0",
        "google-protobuf": "^3.11.2",
        "grpc": "^1.24.2",
        "grpc-boom": "^1.0.26",
        "grpc-tools": "^1.8.1",
        "handlebars": "^4.7.1",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.15",
        "mailgun.js": "^2.0.1",
        "moment": "^2.24.0",
        "mongoose": "^5.8.7",
        "nanoid": "^2.1.9",
        "node-cache": "^5.1.0",
        "node-fetch": "^2.6.0",
        "node-input-validator": "^4.1.0",
        "protobufjs": "^6.8.8",
        "rate-limiter-flexible": "^1.2.2",
        "winston": "^3.2.1"
    },
    "devDependencies": {
        "@types/bcrypt": "^3.0.0",
        "@types/csvtojson": "^1.1.5",
        "@types/dotenv": "^8.2.0",
        "@types/google-protobuf": "^3.7.2",
        "@types/handlebars": "^4.1.0",
        "@types/jsonwebtoken": "^8.3.5",
        "@types/lodash": "^4.14.149",
        "@types/mocha": "^5.2.7",
        "@types/mongoose": "^5.5.41",
        "@types/nanoid": "^2.1.0",
        "@types/node": "^13.1.6",
        "@types/node-cache": "^4.2.5",
        "@types/node-fetch": "^2.5.4",
        "@types/prettier": "^1.19.0",
        "@types/winston": "^2.4.4",
        "del-cli": "^3.0.0",
        "grpc_tools_node_protoc_ts": "^2.5.9",
        "husky": "^4.0.7",
        "lint-staged": "^9.5.0",
        "mocha": "^7.0.0",
        "nodemon": "^2.0.2",
        "prettier": "^1.19.1",
        "ts-node": "^8.6.1",
        "ts-protoc-gen": "^0.12.0",
        "typescript": "^3.7.4",
        "typesync": "^0.6.1"
    },
    "engines": {
        "node": ">=10.16 <12"
    }
}
